    <display-name>displayMovies</display-name>
    <servlet-name>displayMovies</servlet-name>
    <servlet-class>com.psl.controller.displayMovies</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>displayMovies</servlet-name>
